{
  "name": "pretty-git",
  "version": "0.1.0",
  "description": "A small collection of tools to prettify git standard commands on command line.",
  "main": "main.js",
  "bin": {
    "pretty-git": "./main.js"
  },
  "scripts": {
    "build": "gulp && npm run clean-package",
    "build:qa": "npm test && gulp && npm run clean-package",
    "clean-package": "node ./buildtools/cleanup-package.js",
    "exe:js": "node ./src/main.js",
    "exe:ts": "ts-node ./src/main.ts",
    "watch": "watch 'npm run build' ./src",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "test": "ts-node node_modules/jasmine/bin/jasmine"
  },
  "keywords": [],
  "author": "Christian Silfang (comcy)",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jasmine": "^3.3.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.9",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-trx-reporter": "^2.2.2",
    "jasmine-ts": "^0.3.0",
    "node-sass": "^4.10.0",
    "standard-version": "^4.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "minimist": "^1.2.0"
  },
  "peerDependencies": {
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "minimist": "^1.2.0"
  },
  "repository": "https://github.com/comcy/pretty-git.git"
}